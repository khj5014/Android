        <item name="colorPrimary">#93B7D9</item>
        <item name="colorPrimaryVariant">#93B7D9</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#93B7D9</item>
